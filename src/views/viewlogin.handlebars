<main id="loginMain">
  <section class="loginSection">
    <div class="loginDiv">
      <span class="logo"></span>
      <h1 class="loginTitle">Inicio de Sesion</h1>
      <form class="loginForm" id="loginForm">
        <label for="email">Correo</label><br />
        <input
            type="email"
            name="email"
            id="email"
            placeholder="introduzca su correo electronico"
            required
            />
        <br />
        <label for="password">Contrasena</label>
        <br />
        <input
            type="password"
            name="password"
            id="password"
            placeholder="introduzca su contrasena"
            required
        />
        <br />
            <button class="mt-3" type="submit">login</button>
    </form>
    <div class="d-flex">
        <a href="/register" class="m-2" style="color:black"><button>Resgistrate aqui</button></a>
        <a href="/api/session/github" class="m-2"><button>Entra con Github</button></a>
    </div>
    </div>
  </section>
</main>
<script src="/js/login.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

